{
  "name": "vue-project",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "start": "run-s start:*",
    "start:api": "pnpm build:api",
    "start:dev": "run-p dev:*",
    "dev:esbuild": "pnpm build:api --watch",
    "dev:vercel": "vercel dev",
    "dev:vite": "vite --host",
    "test:unit": "vitest",
    "test:e2e": "playwright test",
    "build": "run-s build:*",
    "build:api": "esbuild src/api/*.ts --bundle --sourcemap --format=esm --platform=node --outdir=api",
    "build:check": "vue-tsc --build --force",
    "build:vite": "vite build",
    "preview": "vite preview",
    "lint": "run-s lint:*",
    "lint:format": "prettier --write src/",
    "lint:fix": "eslint . --fix",
    "postversion": "git push --follow-tags"
  },
  "dependencies": {
    "@jsweb/utils": "latest",
    "@vueuse/core": "latest",
    "notiflix": "latest",
    "vue": "latest",
    "vue-router": "latest"
  },
  "devDependencies": {
    "@playwright/test": "latest",
    "@rushstack/eslint-patch": "latest",
    "@tailwindcss/forms": "latest",
    "@tsconfig/node-lts": "latest",
    "@types/jsdom": "latest",
    "@types/node": "latest",
    "@vitejs/plugin-vue": "latest",
    "@vue/eslint-config-prettier": "latest",
    "@vue/eslint-config-typescript": "latest",
    "@vue/test-utils": "latest",
    "@vue/tsconfig": "latest",
    "autoprefixer": "latest",
    "cssnano": "latest",
    "esbuild": "latest",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "latest",
    "jsdom": "latest",
    "lefthook": "latest",
    "npm-run-all2": "latest",
    "postcss": "latest",
    "prettier": "latest",
    "tailwindcss": "latest",
    "typescript": "latest",
    "vercel": "latest",
    "vite": "latest",
    "vitest": "latest",
    "vue-tsc": "latest"
  }
}
