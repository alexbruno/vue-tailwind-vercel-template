{
  "name": "vue-project",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "start": "netlify dev",
    "dev": "vite --host",
    "test:unit": "vitest",
    "test:e2e": "playwright test",
    "build": "run-s build:*",
    "build:check": "vue-tsc --build --force",
    "build:vite": "vite build",
    "preview": "vite preview",
    "lint": "run-s lint:*",
    "lint:format": "prettier --write src/",
    "lint:fix": "eslint . --ext .vue,.js,.ts --fix --ignore-path .gitignore",
    "postversion": "git push --follow-tags"
  },
  "dependencies": {
    "@jsweb/utils": "latest",
    "notiflix": "latest",
    "vue": "latest",
    "vue-router": "latest"
  },
  "devDependencies": {
    "@netlify/functions": "latest",
    "@playwright/test": "latest",
    "@rushstack/eslint-patch": "latest",
    "@tailwindcss/forms": "latest",
    "@tsconfig/node-lts": "latest",
    "@types/express": "latest",
    "@types/jsdom": "latest",
    "@types/markdown-it": "latest",
    "@types/node": "latest",
    "@vitejs/plugin-vue": "latest",
    "@vue/eslint-config-prettier": "latest",
    "@vue/eslint-config-typescript": "latest",
    "@vue/test-utils": "latest",
    "@vue/tsconfig": "latest",
    "@vueuse/core": "latest",
    "autoprefixer": "latest",
    "cssnano": "latest",
    "esbuild": "latest",
    "eslint": "latest",
    "eslint-plugin-vue": "latest",
    "express": "latest",
    "jsdom": "latest",
    "netlify-cli": "latest",
    "npm-run-all2": "latest",
    "postcss": "latest",
    "prettier": "latest",
    "serverless-http": "latest",
    "tailwindcss": "latest",
    "typescript": "latest",
    "vite": "latest",
    "vitest": "latest",
    "vue-tsc": "latest"
  }
}
